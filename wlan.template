# To be installed into /etc/netctl/wlan.template

Description='Basic Wifi config with conf read from /boot/wlan.conf'
Interface=wlan0
Connection=wireless
Security=wpa

IP=dhcp

# Prepend hexadecimal keys with \"
# If your key starts with ", write it as '""<key>"'
# See also: the section on special quoting rules in netctl.profile(5)
# Uncomment this if your ssid is hidden
#Hidden=yes
